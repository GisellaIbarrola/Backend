{{#each products}}
  <li class='card'>
    <h1>{{product.title}}</h1>
    <p>{{product.description}}</p>
    <p>${{product.price}}</p>
    <p>category: {{product.category}}</p>
    <img src={{product.thumbnail}} alt={{product.title}} />
    <p>code: {{product.code}}</p>
    <p>stock: {{product.stock}}</p>
  </li>
{{/each}}